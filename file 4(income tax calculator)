#include<stdio.h>
#include<conio.h>
int main(){
	float income,tax;
	printf("Enter the annual income");
	scanf("%f",&income);
	if(income<=250,000){
	printf("tax=0");}
	else if(income<=500,000){
	printf("income=tax*5%");}
	else if(income<=1,000,000){
	printf("income=tax*10%");}
	else if (income >=1000000){
	printf("income=tax*15%");}
	else if(income>1000000)
	{
	printf("total income is 62500");}
	return 0;
}